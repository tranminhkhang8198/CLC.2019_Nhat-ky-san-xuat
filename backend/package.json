{
  "name": "api",
  "version": "1.0.0",
  "description": "Create backend restful service for Tree planting diary",
  "main": "index.js",
  "keywords": [
    "nodejs",
    "expressjs",
    "eslint",
    "prettier",
    "docker",
    "mongodb",
    "boilerplate",
    "api",
    "apidoc"
  ],
  "scripts": {
    "start": "nodemon index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs": "npm run apidoc && npm run postdoc",
    "apidoc": "apidoc -e \"node_modules/\" -o docs/apidoc/",
    "postdoc": "open-cli docs//apidoc/index.html",
    "migrate_up": "./node_modules/.bin/migrate-mongo up --file ./config/migrate-mongo-config.js",
    "migrate_down": "./node_modules/.bin/migrate-mongo down --file ./config/migrate-mongo-config.js"
  },
  "author": "vanloiidk@gmail.com",
  "license": "ISC",
