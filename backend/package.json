    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6",
    "helmet": "^3.21.3",
    "http-status": "^1.4.2",
    "immutable": "^4.0.0-rc.12",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongodb": "^3.3.4",
    "mongoose": "^5.7.10",
